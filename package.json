{
  "name": "electron-antd",
  "productName": "electron-antd",
  "version": "0.0.1",
  "description": "Build Desktop applications with Electron + Parcel + React + Antd",
  "main": "main.js",
  "scripts": {
    "start": "npm run electron",
    "electron": "electron main",
    "electron-prod": "NODE_ENV=production electron main ",
    "parcel-production": "parcel build ./index.html -d app --public-url ./",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "antd": "^3.4.4",
    "axios": "^0.18.0",
    "brace": "^0.11.1",
    "execa": "^0.9.0",
    "pretty-data": "^0.40.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-ace": "^5.10.0",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "devtron": "^1.4.0",
    "electron": "^1.8.6",
    "electron-builder": "^20.11.1",
    "electron-debug": "^1.4.0",
    "electron-react-devtools": "^0.5.3",
    "less": "^2.7.3",
    "parcel-bundler": "^1.7.1"
  },
  "build": {
    "appId": "br.com.maaarkin.electron-antd",
    "files": [
      "!dist${/*}",
      "!.cache${/*}",
      "!src${/*}",
      "!.gitignore"
    ]
  },
  "keywords": [],
  "author": "Marcos Filho <@maaarkin>",
  "license": "ISC"
}
